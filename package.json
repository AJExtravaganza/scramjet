{
  "name": "scramjet",
  "version": "4.18.12",
  "description": "Lightweight and real-time data functional stream programming framework like event-stream, written in ES6 using async await with multi-threading and typescript support",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.mjs",
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "doc": "docs",
    "example": "samples"
  },
  "keywords": [
    "stream",
    "es6",
    "es7",
    "async",
    "await",
    "multithreaded",
    "threads",
    "transform",
    "transmux",
    "mux",
    "merge",
    "livedata",
    "realtime",
    "filter",
    "map",
    "apache",
    "spark",
    "apex",
    "flink",
    "highland",
    "log",
    "functional",
    "ts",
    "typescript",
    "parser"
  ],
  "scripts": {
    "build": "rollup -c ./.config/rollup-build.js && npm run copy_runtimes",
    "test": "npm run build && gulp fulltest",
    "quicktest": "npm run build && gulp test",
    "preversion": "npm run build && gulp prerelease",
    "docs": "gulp docs",
    "lint": "gulp lint",
    "copy_runtimes": "gulp copy_runtimes"
  },
  "homepage": "https://scramjet.eu/",
  "repository": {
    "type": "git",
    "url": "https://github.com/signicode/scramjet.git"
  },
